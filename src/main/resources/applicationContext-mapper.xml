<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<!-- - Application context definition for JPetStore's business layer. - Contains 
	bean references to the transaction manager and to the DAOs in - dataAccessContext-local/jta.xml 
	(see web.xml's "contextConfigLocation"). -->
<beans default-autowire="byName">		<bean id="xdCustcarinfoMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">		<property name="mapperInterface" value="com.hongte.weixin.mybatis.map.XdCustcarinfoMapper" />		<property name="sqlSessionFactory" ref="sqlSessionFactory" />	</bean>		<bean id="tCarMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">		<property name="mapperInterface" value="com.hongte.weixin.mybatis.map.TCarMapper" />		<property name="sqlSessionFactory" ref="sqlSessionFactory" />	</bean>		<bean id="avwFileprocessMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">		<property name="mapperInterface" value="com.hongte.weixin.mybatis.map.AvwFileprocessMapper" />		<property name="sqlSessionFactory" ref="sqlSessionFactory" />	</bean>		<bean id="xdKkdetailMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">		<property name="mapperInterface" value="com.hongte.weixin.mybatis.map.XdKkdetailMapper" />		<property name="sqlSessionFactory" ref="sqlSessionFactory" />	</bean>		<bean id="avwXdOtherMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">		<property name="mapperInterface" value="com.hongte.weixin.mybatis.map.AvwXdOtherMapper" />		<property name="sqlSessionFactory" ref="sqlSessionFactory" />	</bean>		<bean id="xdDkdetailtobankMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">		<property name="mapperInterface" value="com.hongte.weixin.mybatis.map.XdDkdetailtobankMapper" />		<property name="sqlSessionFactory" ref="sqlSessionFactory" />	</bean>	</beans>
